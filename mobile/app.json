{
  "expo": {
    "name": "finPal",
    "slug": "finpal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/finPal.png",
    "userInterfaceStyle": "automatic",
    "scheme": "finpal",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.palstack.finpal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/finPal.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "io.palstack.finpal"
    },
    "web": {
      "favicon": "./assets/finPal.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
